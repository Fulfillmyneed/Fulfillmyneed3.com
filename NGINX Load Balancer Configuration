upstream backend_servers {
    least_conn;
    server 10.0.1.1:5000;
    server 10.0.1.2:5000;
    server 10.0.1.3:5000;
}

server {
    listen 80;
    server_name api.fulfillme.com;
    
    location / {
        proxy_pass http://backend_servers;
        # ... rest of proxy settings
    }
}